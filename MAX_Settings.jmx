            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import java.util.concurrent.LinkedBlockingQueue;

//Достаем очередь из проперти
	LinkedBlockingQueue tokenQueue = props.get(&quot;tokenQueue_prop&quot;)
	log.info(&quot;Размер очереди в TG MAIN = &quot;+tokenQueue.size())

//Достаем мапу tokenMap из очереди tokenQueue
	def tokenMap = tokenQueue.take()

//Проверяем текущее время меньше чем время протухания токена? Если да, то возвращаем токен обратно в очередь для переиспользования, если нет - возвращаемся к началу цикла while.	
	def gotTkn = true
	while (gotTkn){
		if (System.currentTimeMillis() &lt; tokenMap[&apos;expiredTime&apos;]) {
		tokenQueue.add(tokenMap)
		gotTkn = false
		vars.put(&quot;JWT&quot;, tokenMap[&apos;token&apos;])
		}
	}	
	</stringProp>
