        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler1" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.concurrent.LinkedBlockingQueue;
import java.time.LocalTime;
import java.time.ZoneId;


//Создаем очередь tokenQueue для токенов 
	LinkedBlockingQueue tokenQueue = new LinkedBlockingQueue();

//Создаем мапу для токена и переменной со свременем протухания
	def tokenMap = [:]
	
//Получили токен и положили в tokenMap с ключом token
	tokenMap[&apos;token&apos;] = vars.get(&quot;JWT&quot;)

//Для логирования времени и отладки токена
	LocalTime localTime = LocalTime.now(ZoneId.of(&quot;GMT+03:00&quot;))	

//Зафиксировали время протухания этого токена в tokenMap с ключом expiredTime 
	tokenMap[&apos;expiredTime&apos;] = System.currentTimeMillis() + 180_000
	log.info(&quot;Время &quot;+localTime.toString()+&quot;  Sampler1_JWT - &quot;+vars.get(&quot;JWT&quot;))
	
//Положили этот tokenMap в очередь tokenQueue и затем в проперти 
	tokenQueue.put(tokenMap)
	props.put(&quot;tokenQueue_prop&quot;,tokenQueue)

// Выводим в лог проперти, которые передали в Arrivals Thread Group с помощью баш скрипта
log.info(&quot;Target Rate (arrivals/sec) - &quot; + props.get(&quot;P_TargetRate&quot;))
log.info(&quot;Ramp-up Time (sec) - &quot; + props.get(&quot;P_RampupTime&quot;))
log.info(&quot;Ramp-up Steps Count - &quot; + props.get(&quot;P_RampupStepsCount&quot;))
log.info(&quot;Hold Target Rate Time (sec) - &quot; + props.get(&quot;P_HoldTargetRateTime&quot;))</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
